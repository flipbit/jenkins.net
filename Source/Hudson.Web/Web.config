<?xml version="1.0"?>
<configuration>
	<configSections>
		<section name="windsor" type="Castle.Windsor.Configuration.AppDomain.CastleSectionHandler, Castle.Windsor"/>
	</configSections>
	<appSettings>
		<add key="server" value=""/>
		<add key="username" value=""/>
		<add key="password" value=""/>

<project name="compile-stage">
	<target name="Compile">

		<exec basedir="${root.dir}\Source"
			program="${dotnet35}\msbuild.exe">
			<arg value="${root.dir}\Source\Hudson.sln" />
			<arg value="/t:Build" />
			<arg value="/p:Configuration=Release" />
			<arg value="/verbosity:quiet" />
		</exec>

	</target>
</project>